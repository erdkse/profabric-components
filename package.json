  "scripts": {
    "postinstall": "ngcc --properties es2015 browser module main",
    "start": "concurrently \"nx build web-components --watch\" \"nx serve web-components\"",
    "start:react": "concurrently \"nx build web-components --watch\" \"nx serve react-app\"",
    "start:angular": "concurrently \"nx build web-components --watch\" \"nx serve angular-app\"",
    "build": "nx build web-components && nx build web-components-angular && nx build web-components-react",
    "publish:web": "cd dist/packages/web-components && npm publish --access public",
    "publish:angular": "cd dist/packages/web-components-angular && npm publish --access public",
    "publish:react": "cd dist/packages/web-components-react && npm publish --access public",
    "publish": "npm run publish:web && npm run publish:angular && npm run publish:react"
  },
